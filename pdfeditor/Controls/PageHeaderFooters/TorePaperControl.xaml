<UserControl
	x:Class="pdfeditor.Controls.PageHeaderFooters.TorePaperControl" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:local="clr-namespace:pdfeditor.Controls.PageHeaderFooters"
	HorizontalAlignment="Stretch"
	Height="50"
	SizeChanged="UserControl_SizeChanged">
	<Grid>
		<Path
			x:Name="ContentPath"
			Stroke="#D2D2D2"
			StrokeThickness="1"
			Fill="{Binding Path=ContentBrush, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type local:TorePaperControl}}}" />
	</Grid>
</UserControl>
