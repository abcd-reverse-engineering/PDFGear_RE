<Window
	x:Class="pdfeditor.Controls.Printer.WinPrinterDialog" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" xmlns:local="clr-namespace:pdfeditor.Controls.Printer" xmlns:numUpDown="clr-namespace:pdfeditor.Controls.NumericUpDown" xmlns:commonLibCtrl="clr-namespace:CommonLib.Controls;assembly=CommonLib" xmlns:control="clr-namespace:pdfeditor.Controls" xmlns:localConvert="clr-namespace:pdfeditor.Utils.Converters" xmlns:localprop="clr-namespace:pdfeditor.Properties"
	Title="{StaticResource ApplicationTitle}"
	WindowStartupLocation="CenterOwner"
	SizeToContent="Height"
	ResizeMode="NoResize"
	Background="{DynamicResource WindowsBackground}"
	Foreground="{DynamicResource BtnBrushGrayAndDark}"
	Height="auto"
	Width="1024">
	<FrameworkElement.Resources>
		<ResourceDictionary>
			<localConvert:CenterBorderGapMaskConverter
				x:Key="CenterBorderGapMaskConverter" />
			<Style
				x:Key="{x:Type GroupBox}"
				TargetType="{x:Type GroupBox}">
				<Setter
					Property="BorderBrush"
					Value="#FFAEAEAE" />
				<Setter
					Property="BorderThickness"
					Value="1" />
				<Setter
					Property="Template">
					<Setter.Value>
						<ControlTemplate
							TargetType="{x:Type GroupBox}">
							<Grid
								SnapsToDevicePixels="True">
								<Grid.ColumnDefinitions>
									<ColumnDefinition
										Width="0" />
									<ColumnDefinition
										Width="30" />
									<ColumnDefinition
										Width="Auto" />
									<ColumnDefinition
										Width="*" />
									<ColumnDefinition
										Width="4" />
								</Grid.ColumnDefinitions>
								<Grid.RowDefinitions>
									<RowDefinition
										Height="Auto" />
									<RowDefinition
										Height="Auto" />
									<RowDefinition
										Height="*" />
									<RowDefinition
										Height="4" />
								</Grid.RowDefinitions>
								<Border
									Grid.Row="1"
									Grid.Column="0"
									Grid.RowSpan="3"
									Grid.ColumnSpan="5"
									CornerRadius="4"
									BorderBrush="#00FFFFFF"
									BorderThickness="{TemplateBinding Control.BorderThickness}"
									Background="{TemplateBinding Control.Background}" />
								<Border
									x:Name="Header"
									Grid.Row="0"
									Grid.Column="2"
									Grid.RowSpan="2"
									Padding="3,1,3,0">
									<ContentPresenter
										ContentSource="Header"
										RecognizesAccessKey="True"
										SnapsToDevicePixels="{TemplateBinding UIElement.SnapsToDevicePixels}" />
								</Border>
								<ContentPresenter
									Grid.Row="2"
									Grid.Column="1"
									Grid.ColumnSpan="3"
									Margin="{TemplateBinding Control.Padding}"
									SnapsToDevicePixels="{TemplateBinding UIElement.SnapsToDevicePixels}" />
								<Border
									Grid.Row="1"
									Grid.Column="0"
									Grid.RowSpan="3"
									Grid.ColumnSpan="5"
									CornerRadius="4"
									BorderThickness="{TemplateBinding Control.BorderThickness}"
									BorderBrush="{DynamicResource {DynamicResource MainViewFontSizeBorder}}">
									<UIElement.OpacityMask>
										<MultiBinding
											Converter="{StaticResource CenterBorderGapMaskConverter}">
											<Binding
												ElementName="Header"
												Path="ActualWidth" />
											<Binding
												Path="ActualWidth"
												RelativeSource="{RelativeSource Self}" />
											<Binding
												Path="ActualHeight"
												RelativeSource="{RelativeSource Self}" />
										</MultiBinding>
									</UIElement.OpacityMask>
									<Border
										BorderThickness="{TemplateBinding Control.BorderThickness}"
										BorderBrush="{TemplateBinding Control.BorderBrush}"
										CornerRadius="3">
										<Border
											BorderThickness="0"
											BorderBrush="#FFFFFFFF"
											CornerRadius="2" />
									</Border>
								</Border>
							</Grid>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>
		</ResourceDictionary>
	</FrameworkElement.Resources>
	<Grid
		Margin="20 10 20 20"
		Row="1">
		<FrameworkElement.Resources>
			<ResourceDictionary>
				<localConvert:UIModeToBorderThicknessConverter
					x:Key="UIModeToBorderThicknessConverter" />
				<Style
					x:Key="Title"
					TargetType="{x:Type TextBlock}">
					<Setter
						Property="FontSize"
						Value="12" />
					<Setter
						Property="VerticalAlignment"
						Value="Center" />
					<Setter
						Property="HorizontalAlignment"
						Value="Left" />
					<Setter
						Property="TextBlock.LineHeight"
						Value="13" />
				</Style>
				<Style
					x:Key="recSpliter"
					TargetType="{x:Type Rectangle}">
					<Setter
						Property="Height"
						Value="1" />
					<Setter
						Property="StrokeThickness"
						Value="0" />
					<Setter
						Property="Fill"
						Value="{DynamicResource PrintWindowPerviewBackground}" />
					<Setter
						Property="VerticalAlignment"
						Value="Center" />
				</Style>
				<Style
					x:Key="comboxPrinter"
					TargetType="{x:Type ComboBox}"
					BasedOn="{StaticResource DefaultComboBoxStyle}">
					<Setter
						Property="ItemContainerStyle">
						<Setter.Value>
							<Style
								TargetType="{x:Type ComboBoxItem}"
								BasedOn="{StaticResource DefaultComboBoxItemStyle}">
								<Setter
									Property="Height"
									Value="28" />
								<Setter
									Property="VerticalContentAlignment"
									Value="Center" />
								<Setter
									Property="FrameworkElement.ToolTip"
									Value="{Binding PrinterName}" />
							</Style>
						</Setter.Value>
					</Setter>
				</Style>
				<localConvert:CopiesToVisibilityConverter
					x:Key="numToVisibility" />
				<localConvert:BooleanToVisibilityConverter
					x:Key="boolToVisibility" />
				<localConvert:IsCollatedToImageSourceConverterr
					x:Key="collateTosource" />
				<localConvert:MulBooleanToEnableConverter
					x:Key="mulboolToEnable" />
				<localConvert:CopiesToBooleanConverter
					x:Key="CopiesToBooleanConverter" />
				<localConvert:LogicalOrConverter
					x:Key="LogicalOrConverter" />
				<Style
					x:Key="rbtnStyle"
					TargetType="{x:Type RadioButton}">
					<Setter
						Property="ToggleButton.IsChecked"
						Value="False" />
					<Setter
						Property="Margin"
						Value="0 0" />
					<Setter
						Property="Background"
						Value="#00FFFFFF" />
					<Setter
						Property="Foreground"
						Value="#555" />
					<Setter
						Property="Template">
						<Setter.Value>
							<ControlTemplate
								TargetType="{x:Type RadioButton}">
								<StackPanel
									Orientation="Horizontal"
									Background="#00FFFFFF"
									IsHitTestVisible="True">
									<Border
										x:Name="bord0"
										CornerRadius="100"
										VerticalAlignment="Center"
										HorizontalAlignment="Center"
										Height="12"
										Width="12"
										BorderThickness="1"
										BorderBrush="#757575"
										Background="#FFFFFFFF">
										<Border
											x:Name="bord"
											CornerRadius="100"
											Height="8"
											VerticalAlignment="Center"
											HorizontalAlignment="Center"
											Width="8"
											Background="#757575" />
									</Border>
									<TextBlock
										Text="{TemplateBinding ContentControl.Content}"
										Margin="5 0 0 0"
										FontSize="12"
										VerticalAlignment="Center" />
								</StackPanel>
								<ControlTemplate.Triggers>
									<Trigger
										Property="IsEnabled"
										Value="True">
										<Setter
											TargetName="bord0"
											Property="Border.BorderBrush"
											Value="#757575" />
										<Setter
											TargetName="bord"
											Property="Border.BorderBrush"
											Value="#757575" />
									</Trigger>
									<Trigger
										Property="IsEnabled"
										Value="False">
										<Setter
											TargetName="bord0"
											Property="Border.BorderBrush"
											Value="#D2D2D2" />
										<Setter
											TargetName="bord"
											Property="Border.BorderBrush"
											Value="#D2D2D2" />
									</Trigger>
									<Trigger
										Property="ToggleButton.IsChecked"
										Value="true">
										<Setter
											TargetName="bord"
											Property="Visibility"
											Value="Visible" />
									</Trigger>
									<Trigger
										Property="ToggleButton.IsChecked"
										Value="False">
										<Setter
											TargetName="bord"
											Property="Visibility"
											Value="Hidden" />
									</Trigger>
								</ControlTemplate.Triggers>
							</ControlTemplate>
						</Setter.Value>
					</Setter>
				</Style>
				<Style
					x:Key="PrintWayRadioButtonStyle"
					TargetType="{x:Type RadioButton}">
					<Setter
						Property="Foreground"
						Value="{DynamicResource TextBrushWhiteAndBlack}" />
					<Setter
						Property="Background"
						Value="{DynamicResource SolidBrushWhiteAndBlack}" />
					<Setter
						Property="Template">
						<Setter.Value>
							<ControlTemplate
								TargetType="{x:Type RadioButton}">
								<Grid
									Background="#00FFFFFF">
									<Border
										x:Name="Border"
										BorderBrush="{DynamicResource TextBrushWhiteAndBlack}"
										BorderThickness="0.5"
										CornerRadius="0"
										Width="73.5"
										Height="22"
										Padding="10,0">
										<ContentPresenter
											HorizontalAlignment="Center"
											VerticalAlignment="Center"
											RecognizesAccessKey="True" />
									</Border>
								</Grid>
								<ControlTemplate.Triggers>
									<Trigger
										Property="ToggleButton.IsChecked"
										Value="True">
										<Setter
											TargetName="Border"
											Value="{DynamicResource MainViewPagesMouseOverBackground}"
											Property="Border.Background" />
									</Trigger>
									<Trigger
										Property="UIElement.IsMouseOver"
										Value="True">
										<Setter
											TargetName="Border"
											Value="{DynamicResource MainViewPagesMouseOverBackground}"
											Property="Border.Background" />
									</Trigger>
									<Trigger
										Property="IsPressed"
										Value="True">
										<Setter
											TargetName="Border"
											Property="Border.Background"
											Value="#FFA9A9A9" />
									</Trigger>
								</ControlTemplate.Triggers>
							</ControlTemplate>
						</Setter.Value>
					</Setter>
				</Style>
				<Style
					x:Key="LayoutRadioButtonStyle"
					TargetType="{x:Type RadioButton}">
					<Setter
						Property="Template">
						<Setter.Value>
							<ControlTemplate
								TargetType="{x:Type RadioButton}">
								<Border
									x:Name="Border"
									Background="#FFFFFFFF"
									BorderBrush="#00FFFFFF"
									BorderThickness="2"
									CornerRadius="0"
									Width="44"
									Height="44"
									Padding="0,0">
									<ContentPresenter
										HorizontalAlignment="Center"
										VerticalAlignment="Center" />
								</Border>
								<ControlTemplate.Triggers>
									<Trigger
										Property="ToggleButton.IsChecked"
										Value="True">
										<Setter
											TargetName="Border"
											Value="{DynamicResource MainViewIndeterminate}"
											Property="Border.Background" />
										<Setter
											TargetName="Border"
											Value="{DynamicResource MainViewIndeterminate}"
											Property="Border.BorderBrush" />
										<Setter
											TargetName="Border"
											Property="Border.BorderThickness"
											Value="2" />
									</Trigger>
									<Trigger
										Property="UIElement.IsMouseOver"
										Value="True">
										<Setter
											TargetName="Border"
											Property="Border.Background"
											Value="#FFD3D3D3" />
										<Setter
											TargetName="Border"
											Property="Border.BorderBrush"
											Value="#FFD3D3D3" />
									</Trigger>
									<Trigger
										Property="IsPressed"
										Value="True">
										<Setter
											TargetName="Border"
											Property="Border.Background"
											Value="#FFA9A9A9" />
										<Setter
											TargetName="Border"
											Property="Border.BorderBrush"
											Value="#FFA9A9A9" />
									</Trigger>
								</ControlTemplate.Triggers>
							</ControlTemplate>
						</Setter.Value>
					</Setter>
				</Style>
			</ResourceDictionary>
		</FrameworkElement.Resources>
		<Grid.ColumnDefinitions>
			<ColumnDefinition
				Width="300" />
			<ColumnDefinition
				Width="340" />
			<ColumnDefinition
				Width="*" />
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition
				Height="*" />
			<RowDefinition
				Height="Auto" />
		</Grid.RowDefinitions>
		<GroupBox
			Grid.Column="2"
			Margin="5,0,0,0"
			BorderThickness="1"
			Height="535"
			BorderBrush="{DynamicResource MainViewFontSizeBorder}"
			Header="{x:Static localprop:Resources.WinPagePreviewLabelContent}">
			<Grid>
				<Grid.RowDefinitions>
					<RowDefinition
						Height="*" />
					<RowDefinition
						Height="40" />
				</Grid.RowDefinitions>
				<local:PrintPreviewControl
					x:Name="PreviewControl"
					Margin="5 5 5 0" />
				<StackPanel
					Grid.Row="1"
					Orientation="Horizontal"
					HorizontalAlignment="Center"
					VerticalAlignment="Center">
					<Button
						Style="{StaticResource DialogButtonStyle}"
						BorderThickness="0"
						MinWidth="0"
						Width="24"
						Height="24"
						Padding="2"
						Margin="8,0"
						Command="{Binding ElementName=PreviewControl, Path=PrevPageCommand}">
						<Viewbox
							Margin="0,3,5,4">
							<Path
								Fill="{DynamicResource TextBrushWhiteAndBlack}"
								Data="F1 M5.625,10 C5.625,9.830729 5.686849,9.684245 5.810547,9.560547 L12.685547,2.685547 C12.809244,2.56185 12.955729,2.5 13.125,2.5 C13.294271,2.5 13.440755,2.56185 13.564453,2.685547 C13.68815,2.809246 13.75,2.95573 13.75,3.125 C13.75,3.294271 13.68815,3.440756 13.564453,3.564453 L7.138672,10 L13.564453,16.435547 C13.68815,16.559244 13.75,16.705729 13.75,16.875 C13.75,17.044271 13.68815,17.190756 13.564453,17.314453 C13.440755,17.43815 13.294271,17.5 13.125,17.5 C12.955729,17.5 12.809244,17.43815 12.685547,17.314453 L5.810547,10.439453 C5.686849,10.315756 5.625,10.169271 5.625,10 Z"
								VerticalAlignment="Center"
								HorizontalAlignment="Center" />
						</Viewbox>
					</Button>
					<control:PageIndexTextBox
						MinWidth="80"
						PageCount="{Binding ElementName=PreviewControl, Path=TotalPage}"
						PageIndex="{Binding ElementName=PreviewControl, Path=CurrentPage, Mode=TwoWay}" />
					<Button
						Style="{StaticResource DialogButtonStyle}"
						BorderThickness="0"
						MinWidth="0"
						Width="24"
						Height="24"
						Padding="2"
						Margin="8,0"
						Command="{Binding ElementName=PreviewControl, Path=NextPageCommand}">
						<Viewbox
							Margin="0,3,4,4">
							<Path
								Fill="{DynamicResource TextBrushWhiteAndBlack}"
								Data="F1 M6.25,16.875 C6.25,16.705729 6.311849,16.559244 6.435547,16.435547 L12.861328,10 L6.435547,3.564453 C6.311849,3.440756 6.25,3.294271 6.25,3.125 C6.25,2.95573 6.311849,2.809246 6.435547,2.685547 C6.559244,2.56185 6.705729,2.5 6.875,2.5 C7.044271,2.5 7.190755,2.56185 7.314453,2.685547 L14.189453,9.560547 C14.31315,9.684245 14.375,9.830729 14.375,10 C14.375,10.169271 14.31315,10.315756 14.189453,10.439453 L7.314453,17.314453 C7.190755,17.43815 7.044271,17.5 6.875,17.5 C6.705729,17.5 6.559244,17.43815 6.435547,17.314453 C6.311849,17.190756 6.25,17.044271 6.25,16.875 Z"
								VerticalAlignment="Center"
								HorizontalAlignment="Center" />
						</Viewbox>
					</Button>
				</StackPanel>
			</Grid>
		</GroupBox>
		<Grid
			ColumnSpan="2">
			<FrameworkElement.Resources>
				<ResourceDictionary>
					<Style
						x:Key="CommonHeaderedControlStyle"
						TargetType="{x:Type HeaderedContentControl}">
						<Setter
							Property="Focusable"
							Value="False" />
						<Setter
							Property="HorizontalContentAlignment"
							Value="Stretch" />
						<Setter
							Property="VerticalContentAlignment"
							Value="Top" />
						<Setter
							Property="Padding"
							Value="0,0,0,12" />
						<Setter
							Property="Template">
							<Setter.Value>
								<ControlTemplate
									TargetType="{x:Type HeaderedContentControl}">
									<Grid
										Margin="{TemplateBinding Control.Padding}">
										<Grid.ColumnDefinitions>
											<ColumnDefinition
												Width="Auto"
												MinWidth="50"
												SharedSizeGroup="CommonHeaderedContentControlHeaderColumn" />
											<ColumnDefinition
												Width="*" />
										</Grid.ColumnDefinitions>
										<ContentPresenter
											ContentSource="Header"
											Margin="0,5,8,0"
											MaxWidth="110"
											HorizontalAlignment="Left"
											VerticalAlignment="{TemplateBinding Control.VerticalContentAlignment}">
											<ContentPresenter.ContentTemplate>
												<DataTemplate>
													<commonLibCtrl:AutoTooltipTextBlock
														HorizontalAlignment="Center"
														Style="{StaticResource ToolbarButtonTextStyle}"
														Text="{Binding}" />
												</DataTemplate>
											</ContentPresenter.ContentTemplate>
										</ContentPresenter>
										<ContentPresenter
											ContentSource="Content"
											Grid.Column="1"
											HorizontalAlignment="{TemplateBinding Control.HorizontalContentAlignment}"
											VerticalAlignment="{TemplateBinding Control.VerticalContentAlignment}" />
									</Grid>
								</ControlTemplate>
							</Setter.Value>
						</Setter>
					</Style>
				</ResourceDictionary>
			</FrameworkElement.Resources>
			<Grid.ColumnDefinitions>
				<ColumnDefinition />
				<ColumnDefinition />
			</Grid.ColumnDefinitions>
			<Grid.RowDefinitions>
				<RowDefinition
					Height="100" />
				<RowDefinition
					Height="255" />
				<RowDefinition
					Height="180" />
			</Grid.RowDefinitions>
			<Border
				Grid.ColumnSpan="2"
				BorderThickness="1"
				BorderBrush="{DynamicResource MainViewFontSizeBorder}"
				CornerRadius="3"
				Padding="10 10 10 0"
				Margin="0 8 5 5">
				<Grid>
					<StackPanel
						Orientation="Vertical">
						<HeaderedContentControl
							Foreground="{DynamicResource BtnBrushGrayAndDark}"
							Width="400"
							HorizontalAlignment="Left"
							Header="{x:Static localprop:Resources.WinPrinterNameContent}"
							Style="{StaticResource CommonHeaderedControlStyle}">
							<Grid>
								<Grid.ColumnDefinitions>
									<ColumnDefinition
										Width="*" />
									<ColumnDefinition
										Width="Auto" />
								</Grid.ColumnDefinitions>
								<ComboBox
									x:Name="cboxPrinterList"
									BorderBrush="{DynamicResource MainViewFontSizeBorder}"
									VerticalAlignment="top"
									Style="{StaticResource comboxPrinter}"
									SelectionChanged="cboxPrinterList_SelectionChanged" />
								<Button
									x:Name="PreferenceBtn"
									Content="{x:Static localprop:Resources.WinPrinterBtnPropertyContent}"
									Padding="8,0"
									VerticalAlignment="Stretch"
									Grid.Column="1"
									Margin="6,0,0,0"
									Style="{StaticResource DialogButtonStyle}"
									Click="PrinterPreferenceButton_Click" />
							</Grid>
						</HeaderedContentControl>
						<HeaderedContentControl
							Foreground="{DynamicResource BtnBrushGrayAndDark}"
							Header="{x:Static localprop:Resources.WinPrinterCopiesContent}"
							Style="{StaticResource CommonHeaderedControlStyle}">
							<Grid
								MinHeight="30">
								<Grid.ColumnDefinitions>
									<ColumnDefinition
										Width="52" />
									<ColumnDefinition
										Width="Auto" />
									<ColumnDefinition
										Width="120" />
									<ColumnDefinition
										Width="Auto" />
								</Grid.ColumnDefinitions>
								<commonLibCtrl:NumberBox
									x:Name="tboxCopies"
									FallbackToValidValueOnError="False"
									Width="52"
									Height="23"
									IsArrowEnabled="True"
									NumberFormat="{}{0:0}"
									SmallChange="1"
									Maximum="1000"
									Minimum="1"
									ValueChanged="tboxCopies_ValueChanged" />
								<CheckBox
									x:Name="chkbPrintCollate"
									Grid.Column="1"
									Margin="40 0 0 0"
									VerticalAlignment="Center"
									HorizontalAlignment="Left"
									Style="{StaticResource DefaultCheckBoxStyle}">
									<TextBlock
										Text="{x:Static localprop:Resources.WinPrinterCollateContent}"
										FontSize="12"
										TextWrapping="Wrap" />
								</CheckBox>
								<Image
									x:Name="PrintCollateImage"
									Grid.Column="2"
									Margin="20 -5"
									HorizontalAlignment="Left"
									Visibility="{Binding ElementName=chkbPrintCollate, Path=Visibility}"
									Source="{Binding ElementName=chkbPrintCollate, Path=IsChecked, Converter='{StaticResource collateTosource}'}" />
								<CheckBox
									x:Name="GrayscaleCheckbox"
									Content="{x:Static localprop:Resources.WinPrinterChbkGrayscaleContent}"
									Margin="20,0,0,0"
									VerticalAlignment="Center"
									HorizontalAlignment="Left"
									Style="{StaticResource DefaultCheckBoxStyle}"
									Grid.Column="3"
									Click="GrayscaleCheckbox_Click" />
							</Grid>
						</HeaderedContentControl>
						<CheckBox
							x:Name="chkbRevertPrint"
							Visibility="Collapsed"
							Content="Reverse page order"
							VerticalAlignment="Center"
							HorizontalAlignment="Left"
							Margin="0,0,0,12"
							Style="{StaticResource DefaultCheckBoxStyle}" />
					</StackPanel>
				</Grid>
			</Border>
			<GroupBox
				Grid.Column="0"
				Header="{x:Static localprop:Resources.PrintWindowRangeGroupName}"
				Grid.Row="1"
				Padding="10 10 10 5"
				BorderBrush="{DynamicResource MainViewFontSizeBorder}"
				Margin="0 5 5 5"
				BorderThickness="{Binding Converter='{StaticResource UIModeToBorderThicknessConverter}'}">
				<Grid>
					<StackPanel
						Orientation="Vertical"
						Margin="0,0,0,0"
						ToggleButton.Checked="PageRange_Checked">
						<RadioButton
							x:Name="rdbtnAllPages"
							Margin="0,0,0,8"
							GroupName="printRange"
							Grid.ColumnSpan="2"
							Content="{x:Static localprop:Resources.WinBtnAllPagesContent}"
							HorizontalAlignment="Left"
							VerticalAlignment="Center"
							Style="{StaticResource DefaultRadioButtonStyle}"
							IsChecked="True" />
						<RadioButton
							x:Name="rdbtnCurrentPage"
							Margin="0,0,0,8"
							GroupName="printRange"
							Content="{x:Static localprop:Resources.WinWatermarkCurrentpageContent}"
							Grid.ColumnSpan="2"
							HorizontalAlignment="Left"
							VerticalAlignment="Center"
							Style="{StaticResource DefaultRadioButtonStyle}" />
						<RadioButton
							x:Name="rdbtnSelectPage"
							Margin="0,0,0,12"
							GroupName="printRange"
							Content="{x:Static localprop:Resources.WinBtnSelectedPagesContent}"
