			<commonLibCtrl:HyperlinkButton
				x:Name="ClassicModeBtn"
				Grid.Column="1"
				Foreground="#EF5B52"
				Content="{x:Static localprop:Resources.WinPrinterBtnClassicModeContent}"
				HorizontalAlignment="Left"
				VerticalContentAlignment="Center"
				Margin="24,0,0,0"
				Cursor="Hand"
				Click="ClassicModeBtn_Click" />
			<Button
				Name="btnCacel"
				Grid.Column="0"
				HorizontalAlignment="Left"
				Style="{StaticResource DialogButtonStyle}"
				Height="32"
				FontSize="12"
				MinWidth="80"
				IsCancel="True"
				Visibility="Collapsed"
				Content="{x:Static localprop:Resources.WinBtnCancelContent}"
				Click="btnCancel_Click" />
			<Button
				Name="btnBatchPrint"
				Grid.Column="3"
				MinWidth="80"
				Style="{StaticResource DialogButtonStyle}"
				FontSize="12"
				Content="{x:Static localprop:Resources.BatchPrintDialogBatchPrintButtonContent}"
				Margin="10 0 0 0"
				Click="btnBatchPrint_Click" />
			<Button
				Name="btnPrint"
				Grid.Column="4"
				MinWidth="80"
				Style="{StaticResource PrimaryDialogButtonStyle}"
				FontSize="12"
				Content="{x:Static localprop:Resources.WinViewToolPrintTooltipText}"
				Margin="10 0 0 0"
				Click="btnPrint_Click" />
		</Grid>
	</Grid>
</Window>
