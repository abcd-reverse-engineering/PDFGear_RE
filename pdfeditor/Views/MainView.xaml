									<Track.IncreaseRepeatButton>
										<RepeatButton
											Command="Slider.IncreaseLarge"
											Style="{StaticResource DefaultRightRepeatButtonStyle}" />
									</Track.IncreaseRepeatButton>
								</Track>
							</Grid>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>
		</ResourceDictionary>
