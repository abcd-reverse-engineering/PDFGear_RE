					Property="RangeBase.Minimum"
					Value="0" />
			</Style>
			<Style
				x:Key="DefaultSliderThumbStyle"
				TargetType="{x:Type Thumb}">
				<Setter
					Property="Width"
					Value="8" />
				<Setter
					Property="Height"
					Value="8" />
				<Setter
					Property="Background"
					Value="{DynamicResource MainViewToolBarBackground}" />
				<Setter
					Property="UIElement.SnapsToDevicePixels"
					Value="True" />
				<Setter
					Property="Template">
					<Setter.Value>
						<ControlTemplate
							TargetType="{x:Type Thumb}">
							<Ellipse
								Width="{TemplateBinding FrameworkElement.Width}"
								Height="{TemplateBinding FrameworkElement.Height}"
								Fill="{TemplateBinding Control.Background}"
								StrokeThickness="2"
								Stroke="#AAAAAA"
								Effect="{StaticResource UnhoverThumbEffect}" />
							<ControlTemplate.Triggers>
								<Trigger
									Property="UIElement.IsMouseOver"
									Value="True">
									<Setter
										Property="UIElement.Effect"
										Value="{StaticResource HoverThumbEffect}" />
									<Setter
										Property="FrameworkElement.Cursor"
										Value="Hand" />
								</Trigger>
								<Trigger
									Property="Thumb.IsDragging"
									Value="True">
									<Setter
										Property="UIElement.Effect"
										Value="{StaticResource DragThumbEffect}" />
								</Trigger>
							</ControlTemplate.Triggers>
						</ControlTemplate>
					</Setter.Value>
				</Setter>
			</Style>
			<Style
				x:Key="BeautifySliderStyle"
				TargetType="{x:Type Slider}"
